@model dynamic

<!-- Duplicate Account Modal -->
<div class="modal fade" id="duplicateAccountModalSector" tabindex="-1" aria-labelledby="duplicateAccountModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-scrollable">
        <div class="modal-content">
            <div class="modal-header bg-danger text-white">
                <h5 class="modal-title" id="duplicateAccountModalLabel">âš  Duplicate Account Number Found</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <p class="text-danger">This account number is already linked to another contract. Please review below:</p>
                <table class="table table-bordered table-striped">
                    <thead>
                        <tr id="duplicateAccountTableHeaders">
                            <!-- Headers will be populated dynamically -->
                        </tr>
                    </thead>
                    <tbody id="duplicateAccountResults">
                        <!-- Results will be populated dynamically -->
                    </tbody>
                </table>
                <div class="text-end fw-bold"><span id="accountDuplicateCount"></span></div>
            </div>
        </div>
    </div>
</div>
